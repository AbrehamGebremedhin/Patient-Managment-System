
@{
    ViewData["Title"] = "Order";
}

<div class="text-center">
    <h2 class="brand-text d-none d-lg-inline-block">Labratory Menu</h2>
    <div class="mb-3">
        <label for="exampleFormControlTextarea1" class="form-label"></label>
        <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Labratory Order" disabled></textarea>
    </div>
    <form class="login-form py-5 w-100" method="get" action="index.html">
        <div class="input-material-group mb-3">
            <input class="input-material" id="login-username" type="text" name="Result" autocomplete="on" required data-validate-field="LabResult">
            <label class="label-material" for="Result">Labratory Result</label>
        </div>
        <button class="btn btn-primary mb-3" id="login" type="submit">Submit</button>
    </form>
</div>

